{
    "name": "countlines",
    "displayName": "Count Code Lines",
    "version": "1.0.0",
    "publisher": "miskamero",
    "engines": {
        "vscode": "^1.61.0"
    },
    "main": "./extension.js",
    "scripts": {
        "package": "vsce package -o out/count-lines-of-code.vsix --yarn --npm"
    },
    "contributes": {
        "commands": [
            {
                "command": "count-lines-of-code.runPowerShellScript",
                "title": "Count Code Lines"
            }
        ],
        "keybindings": [
            {
                "command": "count-lines-of-code.runPowerShellScript",
                "key": "ctrl+alt+z",
                "mac": "cmd+alt+z",
                "when": "editorTextFocus"
            }
        ]
    },
    "activationEvents": [
        "onCommand:count-lines-of-code.runPowerShellScript"
    ],
    "devDependencies": {
        "@types/node": "^20.11.4",
        "vscode": "^1.61.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/miskamero/count-code-lines.git"
    },
    "files": [
        "CountLines.ps1",
        "extension.js",
        "runscript.js",
        "out/**/*"
    ],
    "keywords": [
        "count",
        "code",
        "lines",
        "loc",
        "sloc",
        "cloc",
        "ncloc",
        "vscode",
        "extension",
        "extension-pack",
        "extension-pack-dependencies",
        "extension-pack-extension",
        "extension-pack-keymaps",
        "extension-pack-language",
        "count-lines-of-code",
        "count-lines",
        "count-code-lines",
        "count-code",
        "count-loc",
        "count-sloc",
        "count-cloc",
        "count-ncloc"
    ],
    "license": "MIT",
    "description": "Count Code Lines",
    "icon": "extension/icon.png",
    "images": [
        "extension/icon.png"
    ]
}
